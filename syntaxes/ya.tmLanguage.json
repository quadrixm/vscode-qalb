{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ya",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.ya",
				"match": "\\b(صحيح|زائف|لااحد|مثل|أكد|غيرمتزامن|و|ترقب|اكسر|صنف|استمر|وظيفة|حذف|ولو|آخر|إلا|أخيرا|لأن|من|عالمي|لو|استيراد|في|يساوي|امدا|لا|غيرمحلي|أو|مرر|رفع|إرجع|حاول|بينما|مع|محصو|منطقية|عدد|عشري|خيط)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.ya",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ya",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.html.ي"
}
