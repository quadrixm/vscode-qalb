{
    "name": "Ya",
    "displayName": "ي",
    "description": "Ya Language Support",
    "version": "0.0.1",
    "publisher": "Ya",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
